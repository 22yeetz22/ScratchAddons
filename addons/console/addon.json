{
  "name": "Console",
  "description": "Adds blocks to log data to a Scratch console.",
  "info": [
    {
      "type": "notice",
      "text": "Protip: remove logs once you're done debugging",
      "id": "logprotip"
    }
  ],
  "credits": [
    {
      "name": "GrahamSH"
    },
    {
      "name": "TheColaber"
    },
    {
      "name": "jeffalo"
    },
    {
      "name": "eaic"
    }
  ],
  "userscripts": [
    {
      "url": "console.js",
      "matches": ["https://scratch.mit.edu/projects/*"],
      "settingsMatch": {
        "id": "console",
        "value": true
      }
    },
    {
      "url": "pause.js",
      "matches": ["https://scratch.mit.edu/projects/*"],
      "settingsMatch": {
        "id": "pause",
        "value": true
      }
    }
  ],
  "userstyles": [
    {
      "url": "style.css",
      "matches": ["https://scratch.mit.edu/projects/*"]
    }
  ],
  "settings": [
    {
      "name": "Console",
      "id": "console",
      "type": "boolean",
      "default": true
    },
    {
      "name": "Pause Button",
      "id": "pause",
      "type": "boolean",
      "default": true
    }
  ],
  "tags": ["editor", "recommended"],
  "enabledByDefault": true,
  "l10n": true
}
